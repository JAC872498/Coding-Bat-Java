/**/

public int sumHeights2(int[] heights, int start, int end){
  int height=heights[start];
  int alt=0;
  for(int x=start+1;x<=end;x++){
    if(heights[x]>height){
      alt+=((Math.abs(heights[x]-height))*2);
      height=heights[x];
    }else{
      alt+=(Math.abs(heights[x]-height));
      height=heights[x];
    }
  }
  return alt;
}
