/**/

public int commonTwo(String[] a, String[] b) {
  int aNum=0,bNum=0,diff=0;
  while(aNum<a.length&&bNum<b.length){
    if(a[aNum].compareTo(b[bNum])==0){
      diff+=1;
      while(a[aNum].equals(a[aNum+1])){
        aNum+=1;
      }
      while(b[bNum].equals(b[bNum+1)){
        bNum+=1;
      }
    }else if(a[aNum].compareTo(b[bNum])<0){
      aNum+=1;
    }else{
      bNum+=1;
    }
  }
  return diff;
}
