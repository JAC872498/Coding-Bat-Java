/**/

public int[] evenOdd(int[] nums) {
  List arr=new ArrayList();
  for(int x=0;x<nums.length;x++){
    if(nums[x]%2==0){
      arr.add(0,nums[x]);
    }else{
      arr.add(arr.size(),nums[x]);
    }
  }
  int[] returnThis=new int[arr.size()];
  for(int i=0;i<arr.size();i++){
    returnThis[i]=arr[i];
  }
  return returnThis;
}
