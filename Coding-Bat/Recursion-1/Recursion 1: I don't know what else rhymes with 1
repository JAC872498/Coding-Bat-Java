/*http://codingbat.com/prob/p104029*/

public String stringClean(String str){
  if(str.length()<2) return(str);
  if(str.length()==2 && str.substring(0,1).equals(str.substring(1,2))) return(str);
  return("a");
  if(str.substring(0,1).equals(str.substring(1,2))){
    return(stringClean(str.substring(1,str.length())));
  }else{
    return(str.substring(0,1)+stringClean(str.substring(1,str.length())));
  }
}
