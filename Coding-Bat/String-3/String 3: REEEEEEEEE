/**/

public String notReplace(String str) {
  String[] newArr=str.split(" is ");
  String newStr=String.join(" is not ",newArr);
  if(str.length()<=2){
    if(str.equals("is")) return("is not");
    return(str);
  }
  if((str.substring(0,3)).equals("is ")||(str.substring(0,3)).equals("is-")){
    newStr=newStr.substring(0,3)+"not "+newStr.substring(3,newStr.length());
  }
  if((str.substring(str.length()-3,str.length())).equals(" is")||(str.substring(str.length()-3,str.length())).equals("-is")){
    newStr=newStr.substring(0,newStr.length()-3)+" not"+newStr.substring(newStr.length()-3,newStr.length());
  }
  return(newStr);
}
