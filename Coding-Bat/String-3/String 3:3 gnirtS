public boolean gHappy(String str) {
  String arr[]=str.split("");
  for(int x=0;x<arr.length;x++){
    if(arr[x].equals("g")){
      if(x==0&&arr[x+1].equals("g")) return false;
      else if(x==arr.length-1&&arr[x-1].equals("g")) return false;
      else if()
      }
    }
  }
  return true;
}
  
