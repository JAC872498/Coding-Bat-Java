public String wordAppend(String[] strings) {
  String wrd="";
  Map<String, String> map=new HashMap<String, String>();
  for(int x=0;x<strings.length;x++){
    if(map.containsKey(strings[x])){
      wrd=wrd+strings[x];
      map.remove(strings[x])
    }else{
      map.put(strings[x],strings[x])
    }
  }
}
