public boolean match(String wrd1, String wrd2){
  String[] arr1=wrd1.split("");
  String[] arr2=wrd2.split("");
  for(int x=0;x<arr1.length;x++){
    if(arr1[x].equals(arr2[x])==false) return false;
  }return true;
}

public String withoutString(String base, String remove) {
  String newBase = base.replace(remove, "");
  String[] arr1=base.split("");
  String[] arr2=newBase.split("");
  return newBase;
}
